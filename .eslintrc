{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "arrow-parens": "off",
    "jsx-a11y/anchor-is-valid": "off",
    "react/jsx-filename-extension": "off",
    "no-restricted-syntax": "off",
    "no-plusplus": "off",
    "quote-props": ["error", "consistent"]
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack/development.js"
      }
    }
  },
  "env": {
    "browser": true,
    "jest": true
  }
}
